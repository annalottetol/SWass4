                    rdfs:label "CoberturaDeQueijo"@pt ;
                    
                    rdfs:subClassOf pizza:PizzaTopping ;
                    
                    owl:disjointWith pizza:FishTopping ,
                                     pizza:FruitTopping ,
                                     pizza:HerbSpiceTopping ,
                                     pizza:MeatTopping ,
                                     pizza:NutTopping ,
                                     pizza:SauceTopping ,
